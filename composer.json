{
    "name": "modera/backend-google-analytics-config-bundle",
    "type": "symfony-bundle",
    "description": "Provide UI access to configuration properties from 'analytics' category.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@modera.org",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "modera/config-bundle": "^4.0|^5.0",
        "modera/mjr-security-integration-bundle": "^4.0|^5.0",
        "modera/backend-tools-settings-bundle": "^4.0|^5.0",
        "modera/backend-config-utils-bundle": "^4.0|^5.0",
        "sergeil/expander-bundle": "^1.0"
    },
    "require-dev": {
        "phake/phake": "^2.0",
        "phpunit/phpunit": "^5.5",
        "symfony/symfony": "^4.4|^5.0"
    },
    "autoload": {
        "psr-0": { "Modera\\BackendGoogleAnalyticsConfigBundle": "" }
    },
    "target-dir": "Modera/BackendGoogleAnalyticsConfigBundle",
    "extra": {
        "modera-module": {
            "register-bundle": "Modera\\BackendGoogleAnalyticsConfigBundle\\ModeraBackendGoogleAnalyticsConfigBundle"
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
